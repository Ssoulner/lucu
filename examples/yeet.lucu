effect throwint {
	fun throw() !
}

effect throwstr {
	fun throw() !
}

fun main() / debug {
	putint(try int {
		try str {
			throwint.throw()
			"unexpected"
		} with throwstr yeets str {

			fun throw() ! {
				yeet "Hello, World!\n"
			}

		}
		69
	} with throwint yeets int {

		fun throw() ! {
			yeet 42
		}
		
	})

	putstr(try str {
		try int {
			throwstr.throw()
			69
		} with throwint yeets int {

			fun throw() ! {
				yeet 42
			}
	
		}
		"unexpected"
	} with throwstr yeets str {

		fun throw() ! {
			yeet "Hello, World!\n"
		}

	})
}
